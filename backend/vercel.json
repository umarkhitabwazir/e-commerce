{
    "installCommand": "npm install --legacy-peer-deps",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
          },
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ]
}
