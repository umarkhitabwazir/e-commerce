{
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "src/index.js",  
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/**.js": {
      "maxDuration": 60  
    }
  },
  "routes": [
    {
        "src":"/(.*)",
        "dest":"/src/public"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}